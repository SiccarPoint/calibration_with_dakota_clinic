# Dakota input file
environment
  tabular_data
    tabular_data_file "dakota_02_nl2sol.dat"

method
  nl2sol

variables
  continuous_design 2
    descriptors "T" "duration_years"
    lower_bounds -10 20
    upper_bounds 2 120
    initial_point -10 120

interface
  fork
    asynchronous
    evaluation_concurrency 3
    parameters_file "params.in"
    results_file "results.out"
    copy_files "template_dir/*"

    analysis_driver "python driver.py"

    work_directory
      named "NL2SOL/run"
      directory_tag
      directory_save
      file_save

responses
  calibration_terms 1
  response_descriptors "rmse"
  numerical_gradients
    method_source dakota
    interval_type forward
  no_hessians


